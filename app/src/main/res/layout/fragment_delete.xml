<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DeleteFragment"
    android:background="@drawable/fondoprueba">

    <TextView
        android:id="@+id/twEliminarVenta"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_gravity="start|top"
        android:layout_marginStart="100dp"
        android:layout_marginTop="90dp"
        android:background="#E6C504"
        android:text="@string/btnEliminarVenta"
        android:textColor="#E8F2FA"
        android:textSize="30sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/eTIDCliente"
        android:layout_width="260dp"
        android:layout_height="79dp"
        android:backgroundTint="@color/white"
        android:ems="10"
        android:inputType="text"
        android:text="@string/eTIDCliente"
        android:textColor="#E8F2FA"
        android:textSize="30sp"
        android:textStyle="bold"
        android:layout_marginStart="70dp"
        android:layout_marginTop="190dp"
        />


    <Button
        android:id="@+id/btnEliminarVenta"
        android:layout_width="285dp"
        android:layout_height="wrap_content"
        android:layout_gravity="start|bottom"
        android:layout_marginStart="60dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#E8F2FA"
        android:text="@string/btnEliminarVenta"
        android:textColor="#E6C504"
        android:textSize="30sp"
        android:textStyle="bold" />

</FrameLayout>